"Icon"="imageres.dll,-5324"

[HKEY_CLASSES_ROOT\DesktopBackground\Shell\WinXclassicMenu\shell\010flyout\command]
@="PowerShell -windowstyle hidden -Command \"Start-Process cmd -Verb RunAs\""

[HKEY_CLASSES_ROOT\DesktopBackground\Shell\WinXclassicMenu\shell\011flyout]
"MUIVerb"="Task Manager"
"Icon"="taskmgr.exe,-30651"
