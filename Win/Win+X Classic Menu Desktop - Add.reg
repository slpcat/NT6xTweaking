Windows Registry Editor Version 5.00

; MajorGeeks.Com
; Add Win+X Classic Menu to Desktop Context Menu
; https://www.majorgeeks.com/content/page/add_winx_classic_menu.html

[HKEY_CLASSES_ROOT\DesktopBackground\Shell\WinXclassicMenu]
"icon"="bootux.dll,-1034"
"MUIVerb"="Win+X Classic Menu"
"Position"="Bottom"
"SubCommands"=""

[HKEY_CLASSES_ROOT\DesktopBackground\Shell\WinXclassicMenu\shell\001flyout]
"MUIVerb"="Programs and Features"
"Icon"="appwiz.cpl,-1500"

[HKEY_CLASSES_ROOT\DesktopBackground\Shell\WinXclassicMenu\shell\001flyout\command]
@="control appwiz.cpl"

[HKEY_CLASSES_ROOT\DesktopBackground\Shell\WinXclassicMenu\shell\002flyout]
"MUIVerb"="Power Options"
"Icon"="powercfg.cpl,-202"

[HKEY_CLASSES_ROOT\DesktopBackground\Shell\WinXclassicMenu\shell\002flyout\command]
